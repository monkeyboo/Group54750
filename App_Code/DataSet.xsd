                         SalesTicket ON Employees.EmpID = SalesTicket.EmpID INNER JOIN
                         SalesTicketDetails ON SalesTicket.ticketID = SalesTicketDetails.ticketID ON Items.itemID = SalesTicketDetails.itemID
GROUP BY SalesTicket.EmpID, SalesTicket.date, Items.ItemName, SalesTicketDetails.UnitPrice, Employees.Fname, Employees.Lname</CommandText>
