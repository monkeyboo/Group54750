                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        SalesTicket.EmpID AS EmployeeID, Employees.Fname + ' ' + Employees.Lname AS Name, format(SalesTicket.date, 'MM/dd/yy') AS Date, Items.ItemName AS Item, SalesTicketDetails.UnitPrice AS Price, 
                         CAST(SUM(SalesTicketDetails.UnitPrice * (Employees.CommissionRate / 100)) AS decimal(8, 2)) AS Commission
FROM            Items INNER JOIN
                         PayScales INNER JOIN
